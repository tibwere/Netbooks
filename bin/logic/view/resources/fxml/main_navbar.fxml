<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>

<HBox fx:id="main" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="logic.view.navbar.MainNavbarGC">
   <children>
      <Button fx:id="homeBtn" mnemonicParsing="false" onAction="#goToHome" text="HOME">
         <graphic>
            <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/home.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="findBtn" mnemonicParsing="false" text="FIND STORE">
         <graphic>
            <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/find_store.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="forumBtn" mnemonicParsing="false" text="FORUM">
         <graphic>
            <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/forum.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
   </children>
</HBox>
